    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "antd": "^5.4.7",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "markdown-to-jsx": "^7.2.0",
